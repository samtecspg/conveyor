{
    "name": "conveyor-api",
    "version": "1.4.2",
    "description": "Conveyor API",
    "main": "start.js",
    "scripts": {
        "lint": "lab -dL",
        "lint:fix": "lab -dL --lint-fix",
        "test": "lab --lint --timeout 10000",
        "test:coverage": "lab --lint --coverage",
        "start": "npm-run-all --parallel start:server",
        "start:server": "node start.js",
        "precommit": "npm run git:hook",
        "prepush": "npm run git:hook",
        "git:hook": "lab --lint"
    },
    "keywords": [
        "nodejs",
        "hapi",
        "elasticsearch",
        "kibana",
        "ingest",
        "api"
    ],
    "author": "Luis Malav√© <luis.malave@samtec.com> (https://github.com/malave)",
    "license": "SEE LICENSE IN LICENSE.md",
    "devDependencies": {
        "code": "^4.1.0",
        "husky": "^0.13.4",
        "lab": "^15.4.5",
        "npm-run-all": "^4.1.2"
    },
    "dependencies": {
        "async": "^2.6.0",
        "blipp": "^2.3.0",
        "boom": "^4.3.1",
        "dotenv": "^4.0.0",
        "elasticsearch": "^13.3.1",
        "h2o2": "^5.4.0",
        "handlebars": "^4.0.11",
        "hapi": "^16.6.3",
        "hapi-swagger": "^7.10.0",
        "hoek": "^4.2.1",
        "inert": "^4.2.1",
        "joi": "^10.6.0",
        "moment": "^2.21.0",
        "node-red": "^0.20.8",
        "node-red-embedded-start": "^0.1.6",
        "request": "^2.83.0",
        "vision": "^4.1.1"
    },
    "eslintConfig": {
        "extends": "eslint-config-hapi",
        "rules": {
            "quotes": [
                "error",
                "single"
            ]
        }
    }
}
